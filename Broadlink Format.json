  - Platform: state
    entity_id: binary_sensor.garage_motion
    to: 'on'
  condition
  - Condition: Statw
    entity_id: switch.garage_lights
    state: 'off'
  action:
    service: switch.garage_lights
    data:
      entity_id: switch.garage_lights
