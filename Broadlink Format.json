          device_class: door
          optimistic: false
          qos: 1
          retain: false
>>>automations.yaml
     - alias: Reset garagePIR state
        hide_entity: true
        inintial_state: 'on'
        trigger:
        - platform: state
            entity_id: binary_sensor.garage.motion
            from: 'off'
            to: 'on'
            for:
              seconds: 5
        action:
        - service: mqtt.publish
          data:
            topic: tele/RF_Bridge/Result
            payload: D54C1Eoff

